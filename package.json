{
  "name": "@adobe/helix-contentsource-connector",
  "version": "1.0.9",
  "private": true,
  "description": "Helix ContentSource Connector",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon",
    "test": " c8 mocha  -i -g 'Post-Deploy'",
    "test-ci": "c8 mocha -i -g 'Post-Deploy' && codecov",
    "test-postdeploy": "mocha -g 'Post-Deploy'",
    "lint": "./node_modules/.bin/eslint .",
    "semantic-release": "semantic-release",
    "build": "hedy -v",
    "deploy": "unset HLX_AWS_ROLE; hedy -v --deploy --test",
    "deploy-sequences": "unset HLX_AWS_ROLE; hedy --no-build -no-hints -l major -l minor --cleanup-patch 1w --cleanup-minor 1m --cleanup-major 1y",
    "deploy-ci": "unset HLX_AWS_ROLE; hedy -v --deploy --test --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci --cleanup-ci 24h",
    "prepare": "husky install"
  },
  "wsk": {
    "target": "aws",
    "name": "helix-services/contentsource-connector@${version}",
    "memory": 512,
    "awsRole": "arn:aws:iam::118435662149:role/helix-service-role-s3-rw",
    "testUrl": "/_status_check/healthcheck.json",
    "static": [
      "views/header.ejs",
      "views/footer.ejs",
      "views/index.ejs",
      "views/scripts.js",
      "views/styles.css"
    ]
  },
  "mocha": {
    "spec": "test/**/*.test.js",
    "require": "test/setup-env.js",
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-contentsource-connector"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-contentsource-connector/issues"
  },
  "homepage": "https://github.com/adobe/helix-contentsource-connector#readme",
  "dependencies": {
    "@adobe/helix-fetch": "3.0.0",
    "@adobe/helix-onedrive-support": "https://github.com/adobe/helix-onedrive-support.git#msal",
    "@adobe/helix-shared-body-data": "1.1.6",
    "@adobe/helix-shared-config": "1.7.2",
    "@adobe/helix-shared-utils": "1.0.4",
    "@adobe/helix-shared-wrap": "1.0.0",
    "@adobe/helix-status": "9.2.4",
    "@adobe/helix-universal": "1.9.1",
    "@adobe/helix-universal-logger": "2.0.0",
    "@aws-sdk/client-s3": "3.45.0",
    "ejs": "3.1.6",
    "googleapis": "92.0.0",
    "mime": "3.0.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.3.1",
    "@adobe/helix-deploy": "5.0.3",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "c8": "7.10.0",
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "codecov": "3.8.3",
    "dotenv": "10.0.0",
    "eslint": "8.5.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.3",
    "husky": "7.0.4",
    "junit-report-builder": "3.0.0",
    "lint-staged": "12.1.4",
    "mocha": "9.1.3",
    "mocha-multi-reporters": "1.5.1",
    "nock": "13.2.1",
    "nodemon": "2.0.15",
    "proxyquire": "2.1.3",
    "semantic-release": "18.0.1"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.cjs": "eslint"
  },
  "nodemonConfig": {
    "exec": "node ./test/dev/server.js",
    "watch": [
      ".env",
      "."
    ],
    "ext": ".js, .cjs, .ejs, .css"
  }
}
